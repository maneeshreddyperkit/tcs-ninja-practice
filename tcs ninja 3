#include<stdio.h>
#include<string.h>
int main()
{
    int i,j=1,m,len,len1;
    char str[50],str1[50];
    scanf("%s",str);
    len=strlen(str);
    for(i=1;i<=len;i++)
    {
        if(str[i]==str[i+1])
        {
            ;
        }
        else
        {
            str1[j]=str[i];
            j++;
        }
    }
    len1=strlen(str1);
    for(m=len;m>=0;m--)
    printf("%c",str1[m]);
    return 0;
}
